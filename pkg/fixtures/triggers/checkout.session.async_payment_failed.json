{
  "_meta": {
    "template_version": 0
  },
  "fixtures": [
    {
      "name": "product",
      "path": "/v1/products",
      "method": "post",
      "params": {
          "name": "myproduct",
          "description": "(created by Stripe CLI)"
      }
    },
    {
      "name": "price",
      "path": "/v1/prices",
      "method": "post",
      "params": {
        "product": "${product:id}",
        "unit_amount": "1500",
        "currency": "eur"
      }
    },
    {
      "name": "checkout_session",
      "expected_error_type": "invalid_request_error",
      "path": "/v1/checkout/sessions",
      "method": "post",
      "params": {
        "success_url": "https://httpbin.org/post",
        "cancel_url": "https://httpbin.org/post",
        "payment_method_types": ["sepa_debit"],
        "mode": "payment",
        "line_items": [
          {
            "price": "${price:id}",
            "quantity": 2
          }
        ],
        "payment_intent_data": {
          "setup_future_usage": "off_session",
          "shipping": {
            "name": "Jenny Rosen",
            "address": {
              "line1": "510 Townsend St",
              "postal_code": "94103",
              "city": "San Francisco",
              "state": "CA",
              "country": "US"
            }
          }
        }
      }
    },
    {
      "name": "payment_page",
      "expected_error_type": "invalid_request_error",
      "path": "/v1/payment_pages/${checkout_session:id}",
      "method": "get"
    },
    {
      "name": "payment_method",
      "expected_error_type": "invalid_request_error",
      "path": "/v1/payment_methods",
      "method": "post",
      "params": {
        "type": "sepa_debit",
        "sepa_debit": {
          "iban": "IT60X0542811101000000123456"
        },
        "billing_details": {
          "email": "stripe@example.com",
          "name": "Jenny Rosen",
          "phone": "1234567890",
          "address": {
            "line1": "71 Crown Street",
            "city": "London",
            "postal_code": "W10 2WB",
            "country": "GB"
          }
        }
      }
    },
    {
      "name": "payment_page_confirm",
      "expected_error_type": "invalid_request_error",
      "path": "/v1/payment_pages/${checkout_session:id}/confirm",
      "method": "post",
      "params": {
        "payment_method": "${payment_method:id}",
        "expected_amount": 3000
      }
    }
  ]
}
